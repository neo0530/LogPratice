{
  //未指定要執行的 type，則程式將執行所有配置中指定的任務
  "Settings": [
    { //intervalSeconds 記錄的間隔時間，記錄當前時間並將其打印到控制台以及記錄到日誌文件中
      //每隔15秒就記錄到log
      "type": 1,
      "intervalSeconds": 15 
    },
    { //scheduledTime 指定的時間到達時記錄當前時間並將其打印到控制台以及記錄到日誌文件。T為日期和時間的分隔。
      //當到指定時間，就做打印
      "type": 2,
      "scheduledTime": "2024-07-27T22:50:45"
    }
  ]
}
